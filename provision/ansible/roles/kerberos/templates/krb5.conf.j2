[libdefaults]
default_realm = GEIGER.COM

###
# MIT Kerberos.
###
krb4_config = /etc/krb.conf
krb4_realms = /etc/krb.realms
kdc_timesync = 1
ccache_type = 4
forwardable = true
proxiable = true

###
# Heimdal Kerberos
###
v4_instance_resolve = false
v4_name_convert = {
    host = {
        rcmd = host
        ftp = ftp
    }
    plain = {
        something = something-else
    }
}
fcc-mit-ticketflags = true

[realms]
GEIGER.COM = {
    kdc = 10.10.0.13
    kdc = 10.10.0.10
    kdc = 10.10.0.11
}

[domain_realm]

[login]
krb4_convert = true
krb4_get_tickets = false
